{
    "manifest_version": 2,
    "name": "Quotivate-Me",
    "version": "1.0",
    "description": "Extension to Motivate People",
    "permissions": [
        "storage",
        "notifications"
    ],
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "options_page":"options.html",
    "icons":{
        "128": "icons/icon.png",
        "48": "icons/icon.png",
        "16": "icons/icon.png"
    },
    "browser_action":{
        "default_icon": "icons/icon.png",
        "default_popup": "index.html"
    },
    "web_accessible_resources": [
        "1.jpg"
    ]
}